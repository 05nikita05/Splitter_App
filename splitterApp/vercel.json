{
  "version": 2,
  "redirects": [
    {
      "source": "/.well-known/vercel/microfrontend-routing",
      "destination": "/",
      "statusCode": 404
    },
    {
      "source": "/.well-known/vercel/*",
      "destination": "/",
      "statusCode": 404
    }
  ],
  "rewrites": [
    {
      "source": "/old-url",
      "destination": "/new-url"
    }
  ]
}
